{
  "name": "pubspec-dependency-opener",
  "displayName": "Pubspec Dependency Opener",
  "description": "The extention helps to open the package in the browser in one click",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml",
		"onCommand:extension.pubspecDependencyOpener"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
				"command": "extension.pubspecDependencyOpener",
				"title": "Pubspec Dependency Open in Dart Packages Site"
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "homepage": "https://github.com/tenraneko/pubspec_dependency_opener",
	"repository": {
		"type": "git",
		"url": "https://github.com/tenraneko/pubspec_dependency_opener.git"
	}
}
